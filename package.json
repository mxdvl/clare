{
  "name": "clare",
  "description": "Clareâ€™s online notebook",
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm build:icons && vite build && pnpm build:md",
    "build:icons": "node src/sharp.js",
    "build:md": "cp 'src/routes/(book)/words/'*.md '.svelte-kit/output/server/entries/pages/(book)/words/'",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "dependencies": {
    "@fontsource/nunito": "5.0.18",
    "@sveltejs/adapter-auto": "3.2.1",
    "@sveltejs/kit": "2.5.10",
    "@sveltejs/vite-plugin-svelte": "3.1.0",
    "@types/marked": "6.0.0",
    "@types/node": "20.12.12",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "eslint": "9.3.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.39.0",
    "gray-matter": "4.0.3",
    "marked": "12.0.2",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "3.2.3",
    "sharp": "0.33.4",
    "svelte": "4.2.17",
    "svelte-actions-resize": "0.1.1",
    "svelte-check": "3.7.1",
    "svelte-preprocess": "5.1.4",
    "tslib": "2.6.2",
    "typescript": "5.4.5",
    "vite": "5.2.11"
  },
  "engines": {
    "node": "20.13.1"
  },
  "packageManager": "pnpm@8.15.8",
  "prettier": {}
}
