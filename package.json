{
  "name": "clare",
  "description": "Clareâ€™s online notebook",
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm build:icons && vite build && pnpm build:md",
    "build:icons": "node src/sharp.js",
    "build:md": "cp 'src/routes/(book)/words/'*.md '.svelte-kit/output/server/entries/pages/(book)/words/'",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "dependencies": {
    "@fontsource/nunito": "5.2.6",
    "@sveltejs/adapter-auto": "6.1.0",
    "@sveltejs/kit": "2.28.0",
    "@sveltejs/vite-plugin-svelte": "5.1.1",
    "@types/node": "22.17.2",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-svelte": "3.11.0",
    "gray-matter": "4.0.3",
    "marked": "16.1.2",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "sharp": "0.34.3",
    "svelte": "5.38.1",
    "svelte-check": "4.3.1",
    "svelte-preprocess": "6.0.3",
    "tslib": "2.8.1",
    "typescript": "5.9.2",
    "vite": "6.3.5"
  },
  "engines": {
    "node": "22.18.0"
  },
  "packageManager": "pnpm@10.14.0",
  "prettier": {},
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "svelte-preprocess"
    ]
  }
}
