{
  "name": "clare",
  "description": "Clareâ€™s online notebook",
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm build:icons && vite build && pnpm build:md",
    "build:icons": "node src/sharp.js",
    "build:md": "cp 'src/routes/(book)/words/'*.md '.svelte-kit/output/server/entries/pages/(book)/words/'",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "dependencies": {
    "@fontsource/nunito": "5.2.5",
    "@sveltejs/adapter-auto": "3.3.1",
    "@sveltejs/kit": "2.18.0",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@types/marked": "6.0.0",
    "@types/node": "22.13.8",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "9.21.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.46.1",
    "gray-matter": "4.0.3",
    "marked": "12.0.2",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "3.3.3",
    "sharp": "0.33.5",
    "svelte": "4.2.19",
    "svelte-actions-resize": "0.1.1",
    "svelte-check": "3.8.6",
    "svelte-preprocess": "5.1.4",
    "tslib": "2.8.1",
    "typescript": "5.8.2",
    "vite": "5.4.14"
  },
  "engines": {
    "node": "20.18.3"
  },
  "packageManager": "pnpm@10.5.2",
  "prettier": {}
}
