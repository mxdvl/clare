{
  "name": "clare",
  "description": "Clareâ€™s online notebook",
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm build:icons && vite build && pnpm build:md",
    "build:icons": "node src/sharp.js",
    "build:md": "cp 'src/routes/(book)/words/'*.md '.svelte-kit/output/server/entries/pages/(book)/words/'",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "dependencies": {
    "@fontsource/nunito": "5.0.16",
    "@sveltejs/adapter-auto": "3.1.1",
    "@sveltejs/kit": "1.30.3",
    "@types/marked": "5.0.2",
    "@types/node": "20.11.19",
    "@types/sharp": "^0.31.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.35.1",
    "gray-matter": "4.0.3",
    "marked": "11.2.0",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "3.2.0",
    "sharp": "0.33.2",
    "svelte": "4.2.10",
    "svelte-actions-resize": "^0.1.1",
    "svelte-check": "3.6.4",
    "svelte-preprocess": "5.1.3",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "vite": "5.1.1"
  },
  "engines": {
    "node": "20.11.1"
  },
  "packageManager": "pnpm@8.15.3",
  "prettier": {}
}
