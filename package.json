{
  "name": "clare",
  "description": "Clareâ€™s online notebook",
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm build:icons && vite build && pnpm build:md",
    "build:icons": "node src/sharp.js",
    "build:md": "cp 'src/routes/(book)/words/'*.md '.svelte-kit/output/server/entries/pages/(book)/words/'",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "dependencies": {
    "@fontsource/nunito": "5.2.5",
    "@sveltejs/adapter-auto": "4.0.0",
    "@sveltejs/kit": "2.20.2",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@types/node": "22.13.13",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-svelte": "3.3.3",
    "gray-matter": "4.0.3",
    "marked": "15.0.7",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "3.3.3",
    "sharp": "0.33.5",
    "svelte": "5.25.5",
    "svelte-check": "4.1.5",
    "svelte-preprocess": "6.0.3",
    "tslib": "2.8.1",
    "typescript": "5.8.2",
    "vite": "6.2.3"
  },
  "engines": {
    "node": "22.14.0"
  },
  "packageManager": "pnpm@10.6.5",
  "prettier": {},
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "svelte-preprocess"
    ]
  }
}
